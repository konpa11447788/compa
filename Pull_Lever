-- TPBoat
function TPBoat(p, boat, speedx, stop)
    if stop == nil then
        stop = false
    end
    local Distance = (p.Position - boat.Position).Magnitude
    Speed = speedx
    TweenP = game:GetService("TweenService"):Create(boat, TweenInfo.new(Distance / Speed, Enum.EasingStyle.Linear), {
        CFrame = p
    })
    if stop == true then
        TweenP:Cancel()
    else
        TweenP:Play()
    end
end
-- ============= BodyVelocity
spawn(function()
    pcall(function()
        game:GetService("RunService").Stepped:Connect(function()
            if boat_on then
                if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity") then
                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity")
                        :Destroy()
                end
            elseif FLY then
                if not game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity") then
                    local BodyVelocity = Instance.new("BodyVelocity")
                    BodyVelocity.Name = "BodyVelocity"
                    BodyVelocity.Parent = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
                    BodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
                    BodyVelocity.Velocity = Vector3.new(0, 0, 0)
                end
                if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
                    game.Players.LocalPlayer.Character.Humanoid.Sit = false
                end
                if not Attack_Fa then
                    for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                        if v:IsA("BasePart") then
                            v.CanCollide = false
                        end
                    end
                else
                    wait(.1)
                end
                -- elseif game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity") then
                --     game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity")
                --         :Destroy()
            end
        end)
    end)
end)
