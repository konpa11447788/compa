    if tick() - time_old >= 10 then
        time_old = tick()
        local function SetDescription(description)
            local request = http_request or request or HttpPost or syn.request
            if not request then
                warn("âŒ à¹„à¸¡à¹ˆà¸žà¸šà¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™ request")
                return
            end

            local abcdef = {
                Url = "http://localhost:7963/SetDescription?Account=" .. tostring(game.Players.LocalPlayer.Name),
                Method = "POST",
                Body = description
            }

            request(abcdef)
        end

        local function UpdateDescription()
            local player = game.Players.LocalPlayer
            local race = tostring(player:WaitForChild("Data"):WaitForChild("Race").Value)
            local fragments = tonumber(player.Data.Fragments.Value) or 0
            local parts = {}

            -- âœ… Race
            table.insert(parts, "âœ… " .. race .. " (" .. Race_Int .. ")")

            -- âœ… Valkyrie
            if Valkyrie_Helm_H then
                table.insert(parts, "âœ… Valkyrie")
            end

            -- âœ… Mirror
            if Mirror_Fractal_H then
                table.insert(parts, "âœ… Mirror")
            end

            -- âœ… Pull Lever
            if Pull_Lever_H then
                table.insert(parts, "âœ… Pull Lever")
            end

            -- ðŸ’¸ Fragments
            local formattedFragments
            if fragments >= 1000 then
                formattedFragments = string.format("%.1fK", fragments / 1000):gsub("%.0", "") -- 7000 => 7K
            else
                formattedFragments = tostring(fragments)
            end
            table.insert(parts, "ðŸ’¸ " .. formattedFragments .. " Fragments")

            -- à¸£à¸§à¸¡à¸‚à¹‰à¸­à¸„à¸§à¸²à¸¡à¸—à¸±à¹‰à¸‡à¸«à¸¡à¸”à¹€à¸›à¹‡à¸™à¸šà¸£à¸£à¸—à¸±à¸”à¹€à¸”à¸µà¸¢à¸§
            local description = table.concat(parts, "  ")

            -- à¸ªà¹ˆà¸‡à¹„à¸›à¸¢à¸±à¸‡ local API
            SetDescription(description)
            print("ðŸ“¤ à¸ªà¹ˆà¸‡à¸‚à¹‰à¸­à¸¡à¸¹à¸¥à¹à¸¥à¹‰à¸§:", description)
        end

        UpdateDescription()

    end
