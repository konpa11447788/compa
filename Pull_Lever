local Monster
local function GetMonster(vu, position_point)
    if not position_point then
        position_point = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    end
    pcall(function()
        for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
            if v:IsA('Model') and v:FindFirstChild('Humanoid') and v.Humanoid.Health > 0 and
                (v.HumanoidRootPart.Position - position_point.Position).Magnitude <= vu then
                Monster = v
                return
            end
        end
    end)
end
local function Farm_Cake()
    if game.Workspace.Enemies:FindFirstChild('Cake Prince') or game.ReplicatedStorage:FindFirstChild('Cake Prince') then
        if game.Workspace.Enemies:FindFirstChild('Cake Prince') then
            for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
                if v.Name == 'Cake Prince' and v.Humanoid.Health > 0 then
                    repeat
                        wait(.1)
                        if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                        end
                        TP(v.HumanoidRootPart.CFrame * CFrame.new(0, 30, 0), true)
                        EquipWeapon(Weapon)
                        AttackFunction()
                    until not v.Parent or v.Humanoid.Health <= 0
                end
            end
        elseif game.ReplicatedStorage:FindFirstChild('Cake Prince') then
            TP(game.ReplicatedStorage:FindFirstChild('Cake Prince').HumanoidRootPart.CFrame * CFrame.new(0, 30, 0))
        end
    elseif (game.Players.LocalPlayer.Character.HumanoidRootPart.Position -
        Vector3.new(5270.251953125, 1004.005615234375, 23.6695556640625)).Magnitude >= 2000 then
        repeat
            wait()
            TP(CFrame.new(5270.251953125, 1004.005615234375, 23.6695556640625))
        until (game.Players.LocalPlayer.Character.HumanoidRootPart.Position -
            Vector3.new(5270.251953125, 1004.005615234375, 23.6695556640625)).Magnitude <= 3
    elseif (game.Players.LocalPlayer.Character.HumanoidRootPart.Position -
        Vector3.new(5270.251953125, 1004.005615234375, 23.6695556640625)).Magnitude < 2000 then
        Monster = nil
        for i = 1500, 0, -300 do
            GetMonster(i, CFrame.new(5270.251953125, 1004.005615234375, 23.6695556640625))
        end
        if Monster ~= nil and Monster.Humanoid.Health > 0 then
            local time_check = tick()
            PosMon_X = Monster.HumanoidRootPart.CFrame
            StatrMagnet = true
            repeat
                wait()
                if tick() - time_check >= 4 then
                    Monster.Humanoid.Health = 0
                end
                if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                end
                TP(Monster.HumanoidRootPart.CFrame * CFrame.new(0, 30, 0))
                EquipWeapon(Weapon)
                AttackFunction()
            until not Monster.Parent or Monster.Humanoid.Health <= 0
            StatrMagnet = false
        elseif Monster == nil then
            for i = 1500, 0, -300 do
                GetMonster(i, CFrame.new(5270.251953125, 1004.005615234375, 23.6695556640625))
            end
            if Monster == nil then
                TP(xp)
            end
        end
        local Lp = tostring(string.match(tostring(
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("CakePrinceSpawner")), "%d+"))
        if Lp == 'nil' or Lp == nil then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner", true)
        end
    end
end
FLY = true
local time_old = tick()
for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
    if v:IsA('Tool') and tostring(v.ToolTip) == 'Melee' then
        Weapon = v.Name
    end
end
