local HttpService = game:GetService("HttpService")

local function Get_API(Value)
    -- ðŸ”— URL à¸ˆà¸²à¸ Google Apps Script à¸‚à¸­à¸‡à¸„à¸¸à¸“
    local url =
        "https://script.google.com/macros/s/AKfycbwX1lz6pUmGQTFw5PZ2haKqAZL_nq-dw2U-yrSYW6dY-7tmPc9O91gUhpSRXHzuNlHn/exec"

    local success, response = pcall(function()
        return game:HttpGet(url)
    end)

    if not success then
        warn("âŒ à¸”à¸¶à¸‡à¸‚à¹‰à¸­à¸¡à¸¹à¸¥à¹„à¸¡à¹ˆà¸ªà¸³à¹€à¸£à¹‡à¸ˆ:", response)
        return {}
    end

    local ok, data = pcall(function()
        return HttpService:JSONDecode(response)
    end)

    if not ok then
        warn("âŒ à¹à¸›à¸¥à¸‡ JSON à¹„à¸¡à¹ˆà¸ªà¸³à¹€à¸£à¹‡à¸ˆ:", data)
        print("Raw:", response)
        return {}
    end

    local lower = string.lower(Value)

    -- âœ… Mirage
    if lower == "mirage" and typeof(data["Mirage"]) == "table" then
        print("ðŸŒŒ Mirage JobIds:")
        for i = #data["Mirage"], 1, -1 do
            print("   ", data["Mirage"][i])
        end
        return data["Mirage"]
    end

    -- âœ… Fullmoon
    if lower == "fullmoon" and typeof(data["Fullmoon"]) == "table" then
        print("ðŸŒ• Fullmoon JobIds:")
        for i = #data["Fullmoon"], 1, -1 do
            print("   ", data["Fullmoon"][i])
        end
        return data["Fullmoon"]
    end

    -- âœ… Boss à¹€à¸‰à¸žà¸²à¸° Dough King / rip_indra True Form
    local bossList = {"Dough King", "rip_indra True Form"}
    for _, bossName in ipairs(bossList) do
        if string.lower(bossName) == lower and typeof(data[bossName]) == "table" then
            print("ðŸ‘‘ Boss:", bossName)
            for i = #data[bossName], 1, -1 do
                print("   ", data[bossName][i])
            end
            return data[bossName]
        end
    end

    warn("âš ï¸ à¹„à¸¡à¹ˆà¸žà¸šà¸‚à¹‰à¸­à¸¡à¸¹à¸¥à¸ªà¸³à¸«à¸£à¸±à¸š:", Value)
    return {}
end

