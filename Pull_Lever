    _G.Change_Race = tostring(game.Players.LocalPlayer.Data.Race.Value)
end
if _G.Color_Open == tostring(game.Players.LocalPlayer.Name) then
    while wait(0.5) do
        -- pcall(function()
        if not Open_Color_H then
            local function TP(Pos)
                local xps = game.Players.LocalPlayer and game.Players.LocalPlayer.Character.HumanoidRootPart and
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                if xps then
                    if bi_zaz then
                        bi_zaz = false
                    end
                    local Distance = (Pos.Position - xps).Magnitude
                    pcall(function()
                        Tween = game:GetService("TweenService"):Create(
                            game.Players.LocalPlayer.Character.HumanoidRootPart,
                            TweenInfo.new(Distance / 320, Enum.EasingStyle.Linear), {
                                CFrame = Pos
                            })
                        Tween:Play()
                        if Distance <= 290 then
                            Tween:Cancel()
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos
                        end
                    end)
                end
            end
            repeat
                wait(0.1)
                TP(CFrame.new(-5116.11475, 314.53772, -2966.45215, -0.256015986, -4.60879832e-08, 0.96667254,
                    -1.91044602e-08, 1, 4.26172626e-08, -0.96667254, -7.55705631e-09, -0.256015986))
            until (Vector3.new(-5116.11475, 314.53772, -2966.45215) -
                game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 10

            for i, v in pairs(workspace.Map["Boat Castle"].Summoner.Circle:GetChildren()) do
                if v.Name == 'Part' and tostring(v.BrickColor) == 'Oyster' and tostring(v.Part.BrickColor) ==
                    'Lime green' then
                    Oyster_H = true
                end
            end
            for i, v in pairs(game.workspace.Map["Boat Castle"].Summoner.Circle:GetChildren()) do
                if v.Name == 'Part' and tostring(v.BrickColor) == 'Hot pink' and tostring(v.Part.BrickColor) ==
                    'Lime green' then
                    Hot_pink_H = true
                end
            end
            for i, v in pairs(game.workspace.Map["Boat Castle"].Summoner.Circle:GetChildren()) do
                if v.Name == 'Part' and tostring(v.BrickColor) == 'Really red' and tostring(v.Part.BrickColor) ==
                    'Lime green' then
                    Really_red_H = true
                end
            end
            if not Oyster_H then
                for i, v in pairs(workspace.Map["Boat Castle"].Summoner.Circle:GetChildren()) do
                    if v.Name == 'Part' and tostring(v.BrickColor) == 'Oyster' then
                        if tostring(v.Part.BrickColor) ~= 'Lime green' then
                            local args = {
                                [1] = {
                                    ["StorageName"] = "Snow White",
                                    ["Type"] = "AuraSkin",
                                    ["Context"] = "Equip"
                                }
                            }

                            game:GetService("ReplicatedStorage").Modules.Net:FindFirstChild("RF/FruitCustomizerRF")
                                :InvokeServer(unpack(args))

                            wait(1)
                            repeat
                                wait()
                                if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                                end
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Part.CFrame
                            until tostring(v.Part.BrickColor) == 'Lime green'
                            Oyster_H = true
                        end
                    end
                end
            end
            if not Really_red_H then
                for i, v in pairs(workspace.Map["Boat Castle"].Summoner.Circle:GetChildren()) do
                    if v.Name == 'Part' and tostring(v.BrickColor) == 'Really red' then
                        if tostring(v.Part.BrickColor) ~= 'Lime green' then
                            local args = {
                                [1] = {
                                    ["StorageName"] = "Pure Red",
                                    ["Type"] = "AuraSkin",
                                    ["Context"] = "Equip"
                                }
                            }

                            game:GetService("ReplicatedStorage").Modules.Net:FindFirstChild("RF/FruitCustomizerRF")
                                :InvokeServer(unpack(args))
                            wait(1)
                            repeat
                                wait()
                                if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                                end
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Part.CFrame
                            until tostring(v.Part.BrickColor) == 'Lime green'
                            Really_red_H = true
                        end
                    end
                end
            end
            if not Hot_pink_H then
                for i, v in pairs(workspace.Map["Boat Castle"].Summoner.Circle:GetChildren()) do
                    if v.Name == 'Part' and tostring(v.BrickColor) == 'Hot pink' then
                        if tostring(v.Part.BrickColor) ~= 'Lime green' then
                            local args = {
                                [1] = {
                                    ["StorageName"] = "Winter Sky",
                                    ["Type"] = "AuraSkin",
                                    ["Context"] = "Equip"
                                }
                            }

                            game:GetService("ReplicatedStorage").Modules.Net:FindFirstChild("RF/FruitCustomizerRF")
                                :InvokeServer(unpack(args))
                            wait(1)
                            repeat
                                wait()
                                if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                                end
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Part.CFrame
                            until tostring(v.Part.BrickColor) == 'Lime green'
                            Hot_pink_H = true
                        end
                    end
                end
            end
            if Hot_pink_H and Really_red_H and Oyster_H then
                Open_Color_H = true
            end
        else
            if not Setting_mem['Old JobID'] or #Setting_mem['Old JobID'] >= 30 then
                Setting_mem['Old JobID'] = {}
                Update_Setting(Save_Setting)
            end
            if not ijoireger then
                ijoireger = true
                table.insert(Setting_mem['Old JobID'], tostring(game.JobId))
                Update_Setting(Save_Setting)
            end

            local HttpService = game:GetService("HttpService")

            -- ðŸ“‚ à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¹‚à¸«à¸¥à¸”à¸‚à¹‰à¸­à¸¡à¸¹à¸¥à¸ˆà¸²à¸ All_Server_God.json
            function LoadAllServerGod()
                local fileName = "All_Server_God.json"

                -- à¸–à¹‰à¸²à¹„à¸¡à¹ˆà¸¡à¸µà¹„à¸Ÿà¸¥à¹Œ à¹ƒà¸«à¹‰à¸ªà¸£à¹‰à¸²à¸‡à¹ƒà¸«à¸¡à¹ˆà¸žà¸£à¹‰à¸­à¸¡à¹€à¸™à¸·à¹‰à¸­à¸«à¸²à¸§à¹ˆà¸²à¸‡
                if not isfile(fileName) then
                    writefile(fileName, HttpService:JSONEncode({}))
                    warn("âš ï¸ à¸ªà¸£à¹‰à¸²à¸‡à¹„à¸Ÿà¸¥à¹Œà¹ƒà¸«à¸¡à¹ˆ:", fileName)
                    return {}
                end

                -- à¸žà¸¢à¸²à¸¢à¸²à¸¡à¸­à¹ˆà¸²à¸™à¹à¸¥à¸°à¹à¸›à¸¥à¸‡ JSON
                local success, data = pcall(function()
                    return HttpService:JSONDecode(readfile(fileName))
                end)

                if success and typeof(data) == "table" then
                    print("âœ… à¹‚à¸«à¸¥à¸” All_Server_God à¸ªà¸³à¹€à¸£à¹‡à¸ˆ à¸ˆà¸³à¸™à¸§à¸™:", #data)
                    return data
                else
                    warn(
                        "âš ï¸ à¸­à¹ˆà¸²à¸™à¹„à¸Ÿà¸¥à¹Œà¹„à¸¡à¹ˆà¸ªà¸³à¹€à¸£à¹‡à¸ˆ à¸«à¸£à¸·à¸­à¸‚à¹‰à¸­à¸¡à¸¹à¸¥à¹„à¸¡à¹ˆà¸–à¸¹à¸à¸•à¹‰à¸­à¸‡ â†’ à¸£à¸µà¹€à¸‹à¹‡à¸•à¹„à¸Ÿà¸¥à¹Œ")
                    writefile(fileName, HttpService:JSONEncode({}))
                    return {}
                end
            end
            local all_server = LoadAllServerGod()
            if all_server then
                for i, jobId in ipairs(all_server) do
                    print("[" .. i .. "]", jobId)
                end
                for i, v in pairs(all_server) do
                    if not table.find(Setting_mem['Old JobID'], tostring(v)) then
                        hoping = false
                        local TeleportService = game:GetService("TeleportService")
                        TeleportService:TeleportToPlaceInstance(tostring(game.PlaceId), tostring(v),
                            game.Players.LocalPlayer)
                        local time_add = tick()
                        repeat
                            wait(0.5)
                        until hoping or tick() - time_add >= 5
                    end

                end
            end
            wait(10)
        end
        -- end)
    end
end
